{
  "private": false,
  "name": "otter-pro",
  "version": "0.3.23",
  "license": "MIT",
  "scripts": {
    "start": "dumi dev",
    "docs:build": "dumi build",
    "docs:deploy": "gh-pages -d docs-dist",
    "build": "father-build",
    "deploy": "npm run docs:build && npm run docs:deploy",
    "prettier": "prettier --write \"**/*.{js,jsx,tsx,ts,less,md,json}\"",
    "release": "npm publish",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage",
    "prepublishOnly": "npm run build"
  },
  "module": "es/index.js",
  "typings": "es/index.d.ts",
  "files": [
    "es",
    "lib"
  ],
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "4.7.0",
    "@types/lodash": "4.14.181",
    "ahooks": "3.3.10",
    "ali-oss": "^6.16.0",
    "antd": "4.23.2",
    "await-to-js": "^3.0.0",
    "axios": "^0.24.0",
    "i18next": "21.6.4",
    "i18next-browser-languagedetector": "6.1.2",
    "lodash": "4.17.21",
    "nprogress": "^0.2.0",
    "nrm": "^1.2.5",
    "react": "^17.0.2",
    "react-aws-s3": "^1.5.0",
    "react-i18next": "11.15.2",
    "request": "2.88.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.15.1",
    "@testing-library/react": "^12.1.2",
    "@types/ali-oss": "^6.16.3",
    "@types/jest": "^27.0.3",
    "@types/nprogress": "^0.2.0",
    "@umijs/fabric": "^2.8.1",
    "@umijs/test": "^3.0.5",
    "autoprefixer": "^10.4.0",
    "babel-plugin-import": "^1.13.3",
    "dumi": "^1.1.0",
    "father-build": "^1.17.2",
    "gh-pages": "^3.0.0",
    "lint-staged": "^10.0.7",
    "postcss": "^8.4.5",
    "prettier": "^2.2.1",
    "tailwindcss": "^3.0.7",
    "yorkie": "^2.0.0"
  }
}
